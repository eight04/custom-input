{
	"env": {
		"es6": true,
		"node": true
	},
	"rules": {
		"no-use-before-define": [2, "nofunc"],
		"semi": [2, "always"],
		"no-console": ["error", {"allow": ["error"]}]
	},
	"extends": [
		"eslint:recommended"
	],
	"overrides": [{
		"files": ["**/rollup.config.js"],
		"parserOptions": {
			"sourceType": "module"
		}
	}]
}
